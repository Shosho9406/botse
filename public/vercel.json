{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/products.html", "destination": "/products.html" },
    { "source": "/admin.html", "destination": "/admin.html" },
    { "source": "/order.html", "destination": "/order.html" },
    { "source": "/payment.html", "destination": "/payment.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
